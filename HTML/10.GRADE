<!DOCTYPE html>
<html>
<head>
  <title>Student Grade Checker</title>
  <script>
    function checkGrade() {
      var marks = document.getElementById("marks").value;
      var grade = "";

      if (marks === "") {
        alert("Please enter marks.");
        return;
      }

      marks = parseFloat(marks);

      if (marks >= 90)
        grade = "A";
      else if (marks >= 80)
        grade = "B";
      else if (marks >= 70)
        grade = "C";
      else if (marks >= 60)
        grade = "D";
      else
        grade = "F";

      document.getElementById("result").innerHTML = "Your Grade is: " + grade;
    }
  </script>
</head>
<body>

  <h2>Student Grade Checker</h2>

  <form onsubmit="event.preventDefault(); checkGrade();">
    Enter Marks: <input type="number" id="marks" min="0" max="100">
    <input type="submit" value="Check Grade">
  </form>

  <h3 id="result"></h3>

</body>
</html>

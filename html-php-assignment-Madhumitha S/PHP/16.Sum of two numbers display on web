<?php
// Function to add two numbers
function addNumbers($num1, $num2) {
    return $num1 + $num2;
}

// Example values
$number1 = 10;
$number2 = 20;

// Call function
$sum = addNumbers($number1, $number2);

// Display result
echo "The sum of $number1 and $number2 is: $sum";
?>

output:
The sum of 10 and 20 is: 30
